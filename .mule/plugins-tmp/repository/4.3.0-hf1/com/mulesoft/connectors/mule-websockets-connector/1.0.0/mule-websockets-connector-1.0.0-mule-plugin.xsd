          <xs:element minOccurs="0" maxOccurs="1" name="default-groups">
            <xs:annotation>
              <xs:documentation>The groups to which newly created sockets should be initially subscribed to</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="default-group">
                  <xs:complexType>
                    <xs:attribute type="mule:expressionString" use="required" name="value"></xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
